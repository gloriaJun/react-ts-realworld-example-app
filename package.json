    "react-dom": "^17.0.2",
    "react-router-dom": "next",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@commitlint/cli": "^12.1.4",
